{
  "name": "nadesiko3",
  "version": "3.3.28",
  "description": "Japanese Programming Language",
  "main": "src/index.mjs",
  "bin": {
    "cnako3": "src/cnako3.mjs",
    "nako3server": "src/nako3server.mjs",
    "nako3edit": "tools/nako3edit/index.mjs"
  },
  "scripts": {
    "start": "node src/nako3server.mjs",
    "nako3edit": "node tools/nako3edit/index.mjs",
    "test": "cross-env TZ=Asia/Tokyo mocha test/common/*.mjs",
    "test:node": "mocha --extension mjs test/node",
    "test:async": "mocha test/async -g aysnc_basic_test",
    "test:common": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless --reporters=mocha test/karma.config.js",
    "test:browser": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless --reporters=mocha test/browser/karma.config.js",
    "test:bundled": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless test/bundled/karma.config.js",
    "test:bundled:watch": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --auto-watch --browsers FirefoxCustom test/bundled/karma.config.js",
    "test:ace-editor": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless test/ace_editor/karma.config.js",
    "test:all": "npm test && npm run test:node && npm run test:browser && npm run test:bundled && npm run test:ace-editor",
    "test:bundled:win": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless,ChromeCustomHeadless,EdgeHeadless,IE test/bundled/karma.config.base.js",
    "build:nako_version": "node src/cnako3.mjs batch/build_wnako_version.nako3",
    "build": "npm run build:nako_version && tsc && npm run build:command && webpack --mode production && node src/cnako3.mjs batch/calc_hash.nako3",
    "tsc": "tsc --watch",
    "build:webpack": "tsc && webpack --mode development",
    "check:command": "node src/cnako3.mjs batch/pickup_command.nako3",
    "build:command": "cross-env REPORT_ERR=1 npm run check:command && node src/cnako3.mjs batch/build_command.nako3",
    "build:win32": "node src/cnako3.mjs installer/make-win32.nako3",
    "build:browsers": "npx browserslist > src/browsers.txt && node src/cnako3.mjs batch/build_browsers.nako3",
    "watch": "webpack --watch --mode development",
    "analyze": "webpack-bundle-analyzer release/stats.json",
    "publish": "echo 'Please exec => npm run build && npm run test:all && npm publish && npm run publish:version'",
    "publish:version": "node src/cnako3.mjs batch/publish_version.nako3",
    "eslint": "eslint . --ext mts,mjs",
    "extlib:clean": "rm -f -r demo/extlib/*",
    "extlib:install": "node src/cnako3.mjs batch/download-extlib.nako3",
    "check_new_version": "node src/cnako3.mjs tools/check_new_version.nako3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3.git"
  },
  "keywords": [
    "script",
    "Japanse Programming Language",
    "Nadesiko",
    "日本語プログラミング言語",
    "なでしこ"
  ],
  "files": [
    "bin",
    "demo",
    "doc",
    "src",
    "test",
    "tools",
    "batch",
    "release"
  ],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3/issues"
  },
  "browserslist": [
    "> 0.5%",
    "> 0.5% in JP",
    "last 2 versions",
    "Firefox ESR",
    "maintained node versions",
    "not dead",
    "not IE 11"
  ],
  "eslintConfig": {
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "curly": [
        "error",
        "all"
      ],
      "quote-props": "off"
    },
    "plugins": [
      "react"
    ]
  },
  "homepage": "https://github.com/kujirahand/nadesiko3#readme",
  "_moduleAliases": {
    "root": ".",
    "nako3": "./src",
    "core": "./node_modules/nadesiko3core/src",
    "utils": "./utils"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.2.1",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.33",
    "@types/opener": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "browserslist": "^4.20.2",
    "chai": "^4.3.6",
    "constants-browserify": "^1.0.0",
    "core-js": "^3.22.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "es6-promise": "^4.2.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.29.4",
    "fetch-mock": "^9.5.0",
    "hotkeys-js": "^3.8.9",
    "karma": "^6.3.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "mocha": "^10.0.0",
    "mocha-css": "^1.0.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "npm-check-updates": "^13.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "pump": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "regenerator-runtime": "^0.13.9",
    "source-map": "^0.7.3",
    "ssri": "^9.0.0",
    "standard": "^16.0.4",
    "stats-webpack-plugin": "^0.7.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.7.1",
    "stylelint-config-standard": "^25.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "testdouble": "^3.16.5",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "nadesiko3core": "^3.3.28",
    "dayjs": "^1.11.1",
    "fs-extra": "^10.1.0",
    "html": "^1.0.0",
    "iconv-lite": "^0.6.3",
    "marked": "^4.0.14",
    "node-fetch": "^3.2.3",
    "opener": "^1.5.2"
  }
}
