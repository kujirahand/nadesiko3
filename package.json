{
  "name": "nadesiko3",
  "version": "3.2.24",
  "description": "Japanese Programming Language",
  "main": "src/index.js",
  "bin": {
    "cnako3": "src/cnako3.js",
    "nako3server": "src/nako3server.js",
    "nako3edit": "tools/nako3edit/run.js"
  },
  "scripts": {
    "test": "cross-env TZ=Asia/Tokyo mocha",
    "test:async": "mocha test/async -g aysnc_basic_test",
    "test:common": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless --reporters=mocha test/karma.config.js",
    "test:browser": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless --reporters=mocha test/browser/karma.config.js",
    "test:bundled": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless test/bundled/karma.config.js",
    "test:bundled:watch": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --auto-watch --browsers FirefoxCustom test/bundled/karma.config.js",
    "test:ace-editor": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless test/ace_editor/karma.config.js",
    "test:all": "npm test && npm run test:browser && npm run test:bundled && npm run test:ace-editor",
    "test:bundled:win": "cross-env TZ=Asia/Tokyo NODE_ENV=development karma start --single-run --browsers FirefoxCustomHeadless,ChromeCustomHeadless,EdgeHeadless,IE test/bundled/karma.config.base.js",
    "start": "node src/nako3server.js",
    "nako3edit": "node src/cnako3.js tools/nako3edit/index.nako3",
    "nako3edit:run": "node tools/nako3edit/run.js",
    "electron": "electron .",
    "build": "node src/cnako3.js batch/build_command.nako3 && webpack --mode production && node src/cnako3.js batch/calc_hash.nako3",
    "build:command": "cross-env REPORT_ERR=1 node src/cnako3.js batch/build_command.nako3",
    "build:electron": "electron-packager . --out release/enako3 --asar --all --overwrite",
    "build:win32": "node src/cnako3.js installer/make-win32.nako3",
    "build:browsers": "node src/cnako3.js batch/build_browsers.nako3",
    "watch": "webpack --watch --mode development",
    "analyze": "webpack-bundle-analyzer release/stats.json",
    "release": "npm run build && npm run test:all && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3.git"
  },
  "keywords": [
    "script",
    "Japanse Programming Language",
    "Nadesiko",
    "日本語プログラミング言語",
    "なでしこ"
  ],
  "files": [
    "bin",
    "demo",
    "doc",
    "src",
    "test",
    "tools",
    "release"
  ],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3/issues"
  },
  "browserslist": [
    "> 0.5%",
    "> 0.5% in JP",
    "last 2 versions",
    "Firefox ESR",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "curly": [
        "error",
        "all"
      ]
    },
    "plugins": [
      "react"
    ]
  },
  "homepage": "https://github.com/kujirahand/nadesiko3#readme",
  "_moduleAliases": {
    "root": ".",
    "nako3": "./src",
    "utils": "./utils"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.1.0",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "core-js": "^3.10.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "es6-promise": "^4.2.8",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "fetch-mock": "^9.11.0",
    "hotkeys-js": "^3.8.3",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.0.1",
    "mocha-css": "^1.0.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "nadesiko3-hoge": "0.0.3",
    "npm-check-updates": "^11.4.1",
    "prop-types": "^15.7.2",
    "pump": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "source-map": "^0.7.3",
    "ssri": "^8.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "testdouble": "^3.16.1",
    "textlint": "^12.0.0",
    "textlint-rule-abbr-within-parentheses": "^1.0.2",
    "textlint-rule-footnote-order": "^1.0.3",
    "textlint-rule-general-novel-style-ja": "^1.3.0",
    "textlint-rule-ja-hiragana-fukushi": "^1.3.0",
    "textlint-rule-ja-hiragana-hojodoushi": "^1.0.4",
    "textlint-rule-ja-hiragana-keishikimeishi": "^1.1.0",
    "textlint-rule-ja-unnatural-alphabet": "^2.0.1",
    "textlint-rule-ng-word": "^1.0.0",
    "textlint-rule-no-dead-link": "^4.7.0",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-prefer-tari-tari": "^1.0.3",
    "textlint-rule-preset-ja-spacing": "^2.0.2",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-preset-jtf-style": "^2.3.8",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.27.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "browserslist": "^4.16.3",
    "caniuse-db": "^1.0.30001208",
    "clipboardy": "^2.3.0",
    "csv-lite-js": "^0.0.7",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "html": "^1.0.0",
    "iconv-lite": "^0.6.2",
    "marked": "^2.0.1",
    "node-fetch": "^2.6.1",
    "opener": "^1.5.2",
    "readline-sync": "^1.4.10",
    "sendkeys-js": "0.0.4",
    "whatwg-fetch": "^3.6.2"
  }
}
